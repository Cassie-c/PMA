<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="463.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ManEnterController">
   <children>
      <Label layoutX="5.0" layoutY="50.0" prefHeight="0.0" text="Name" />
      <TextField fx:id="name" layoutX="168.0" layoutY="45.0" prefHeight="0.0" prefWidth="389.0" promptText="Drug name " />
      <Label layoutX="4.0" layoutY="150.0" text="Strength" />
      <TextField fx:id="strength" layoutX="170.0" layoutY="145.0" prefHeight="25.0" prefWidth="383.0" promptText="strength..." />
      <Label layoutX="4.0" layoutY="200.0" text="Daily Dosage" />
      <TextField fx:id="dailyDosage" layoutX="175.0" layoutY="193.0" prefHeight="25.0" prefWidth="383.0" promptText="Maximum daily dose" />
      <Label layoutX="9.0" layoutY="248.0" text="Tablets" />
      <TextField fx:id="tablets" layoutX="173.0" layoutY="243.0" prefHeight="25.0" prefWidth="389.0" promptText="1,2,3..." />
      <Label layoutX="8.0" layoutY="298.0" text="Pharmaceutical Company" />
      <TextField fx:id="pharmaceuticalCompany" layoutX="172.0" layoutY="293.0" prefHeight="25.0" prefWidth="389.0" promptText="company a" />
      <Label layoutX="4.0" layoutY="100.0" text="Barcode" />
      <TextField fx:id="barcode" layoutX="170.0" layoutY="95.0" prefHeight="25.0" prefWidth="383.0" promptText="Barcode of drug package" />
      <Label layoutX="8.0" layoutY="348.0" text="Conflict of drugs" />
      <TextField fx:id="conflictDrugs" layoutX="171.0" layoutY="348.0" prefHeight="25.0" prefWidth="389.0" promptText="drug1,drug2" />
      <Button layoutX="4.0" layoutY="399.0" mnemonicParsing="false" onAction="#submitAct" prefHeight="31.0" prefWidth="591.0" text="Submit" textFill="#080e65" />
   </children>
</AnchorPane>
